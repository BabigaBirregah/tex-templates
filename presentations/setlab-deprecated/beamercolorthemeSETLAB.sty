\ProvidesPackage{beamercolorthemeSETLAB}
   [2010/12/13 v1.0 Beamer 'SeT Lab' Color Theme]

\mode<presentation>

\definecolor{SETLABwhite}{rgb}{1.0,1.0,1.0}
\definecolor{SETLABblack}{rgb}{0.0,0.0,0.0}
\definecolor{SETLABblue}{rgb}{0.19,0.19,0.59}
\definecolor{SETLABred}{rgb}{0.71,0.31,0.36}
\definecolor{SETLABlightorange}{rgb}{1.0,0.6,0.37}
\definecolor{SETLABdarkgray}{rgb}{0.30,0.30,0.36}

\setbeamercolor{normal text}{fg=SETLABblack}
\setbeamercolor{alerted text}{fg=SETLABlightorange}
\setbeamercolor{example text}{fg=SETLABred}
\setbeamercolor{verse}{fg=SETLABred}
\setbeamercolor{quotation}{fg=SETLABred}
\setbeamercolor{quote}{fg=SETLABred}
\setbeamercolor{semiverbatim}{fg=SETLABdarkgray}
\setbeamercolor{structure}{fg=SETLABred}
\setbeamercolor{background canvas}{bg=SETLABwhite}
\setbeamercolor{author in head/foot}{fg=SETLABred}
\setbeamercolor{title in head/foot}{fg=SETLABred}
\setbeamercolor{date in head/foot}{fg=SETLABred}
\setbeamercolor{author}{fg=SETLABblue}
\setbeamercolor{institute}{fg=SETLABred}
\setbeamercolor{date}{fg=SETLABlightorange}
\setbeamercolor{frametitle}{fg=SETLABwhite}
\setbeamercolor{framesubtitle}{fg=SETLABwhite}
\setbeamercolor{footnote}{fg=SETLABdarkgray}

%%% LISTING EXTENSION
%\definecolor{hellgelb}{rgb}{1,1,0.8}
\definecolor{listingKeys}{rgb}{0,0,1}
\definecolor{listingIdentifier}{rgb}{0,0,0}
\definecolor{listingComments}{rgb}{1,0,0}
\definecolor{listingString}{rgb}{0,0.5,0}

\mode<all>

